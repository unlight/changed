{
  "name": "is-changed",
  "version": "2.1.2",
  "description": "Designed to track dependencies in package.json and check file modification time",
  "main": "lib",
  "typings": "src/index.ts",
  "scripts": {
    "test": "npm run t",
    "clean": "rm -rf lib",
    "build": "npm run compile",
    "compile": "tsc",
    "compile:w": "tsc -w",
    "precompile": "npm run clean",
    "t": "npm run mocha -- src/*.spec.ts",
    "test:w": "npm run mocha -- --watch-extensions ts --watch src/**/*.spec.ts",
    "mocha": "ts-node -F node_modules/mocha/bin/_mocha",
    "debug": "inspect node_modules/ts-node/dist/_bin.js -F \"node_modules/mocha/bin/_mocha\" \"src/*.spec.ts\"",
    "release": "npm run build && np"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [],
  "license": "MIT",
  "devDependencies": {
    "@types/mkdirp": "^0.5.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.26",
    "mocha": "^3.5.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@epam/inject": "^1.0.1",
    "difference-json": "^1.0.6",
    "filenamify": "^2.0.0",
    "mkdirp": "^0.5.1",
    "read-pkg": "^2.0.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unlight/changed.git"
  },
  "bugs": {
    "url": "https://github.com/unlight/changed/issues"
  },
  "homepage": "https://github.com/unlight/changed#readme"
}
